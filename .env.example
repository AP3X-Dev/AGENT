# AG3NT Environment Configuration
# Copy this file to .env and fill in your API keys
# NEVER commit .env to version control!

# =============================================================================
# MODEL PROVIDER CONFIGURATION
# =============================================================================

# Primary model provider: anthropic, openai, openrouter, kimi, google
AG3NT_MODEL_PROVIDER=openrouter

# Model name (provider-specific format)
# Anthropic: claude-sonnet-4-5-20250929, claude-3-opus-20240229
# OpenAI: gpt-4o, gpt-4-turbo
# OpenRouter: anthropic/claude-3.5-sonnet, deepseek/deepseek-chat-v3-0324
# Kimi: moonshot-v1-128k, kimi-latest
# Google: gemini-pro, gemini-pro-1.5
AG3NT_MODEL_NAME=deepseek/deepseek-chat-v3-0324

# =============================================================================
# API KEYS (set the ones for your chosen providers)
# =============================================================================

# OpenRouter - https://openrouter.ai/keys
OPENROUTER_API_KEY=

# OpenRouter model for general use
OPENROUTER_MODEL=deepseek/deepseek-v3.2

# OpenRouter image generation model
OPENROUTER_IMAGE_MODEL=google/gemini-3-pro-image-preview

# Anthropic - https://console.anthropic.com/
ANTHROPIC_API_KEY=

# OpenAI - https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Kimi/Moonshot - https://platform.moonshot.cn/
KIMI_API_KEY=

# Google - https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=

# =============================================================================
# OPTIONAL: TOOL API KEYS
# =============================================================================

# Tavily - Web search - https://tavily.com/
TAVILY_API_KEY=

# E2B - Secure code execution sandbox - https://e2b.dev/
E2B_API_KEY=

# =============================================================================
# OPTIONAL: OBSERVABILITY
# =============================================================================

# LangSmith tracing - https://smith.langchain.com/
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=
# LANGCHAIN_PROJECT=ag3nt

# =============================================================================
# OPTIONAL: WORKSPACE CONFIGURATION
# =============================================================================

# Custom workspace for generated files (images, exports, etc.)
# Defaults to ./workspace in the project root
# DEEPAGENTS_WORKSPACE=C:/path/to/custom/workspace

